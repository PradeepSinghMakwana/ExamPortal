@model IEnumerable<ExamPortal.Models.UnapprovedResult>

<div>
    <table class="table">
        <tr>
            <th>
                Approve
            </th>
            <th>
                @Html.DisplayNameFor(model => model.photo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.unique_name)
            </th>
            <th>
                Class
            </th>
            <th>
                @Html.DisplayNameFor(model => model.subject_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.test_name)
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.CheckBox("",false)
                </td>
                <td>
                    <img src="@Url.Content(item.photo)" alt="IMAGES" />
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.unique_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pretty_Class_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.subject_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.test_name)
                </td>
            </tr>
        }

    </table>

</div>
